<div class="container">
<div class="row">
<div class="col-md-6 col-md-offset-3">
<%= form_with(model: @user, local: true) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>



 

  <div class="actions">
    <%= f.submit "Create my account", class: "btn btn-primary" %>
  </div>
<% end %>
</div>
</div>
</div>