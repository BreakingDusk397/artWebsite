    <ul class="orders">
        <% @orders.each do |order| %>
            <li>
                <%= order.product_id %>
                <%= order.user_id %>
                <%= order.status %>
                <%= order.error_message %>
                <%= order.customer_id %>
                <%= order.price_cents %>
                <%= order.created_at %>
            </li>
        <% end %>
    </ul>

<ul class="pictures">
        <% @pictures.each do |picture| %>
            <li>
                <%= picture.title %>
                <%= picture.description %>
                <%= picture.date %>
                <%= picture.location %>
                <%= picture.price %>
                <%= picture.created_at %>
            </li>
        <% end %>
    </ul>

<%= link_to "New Picture", new_picture_path %>